{
  "name": "et-tutor-web",
  "version": "1.0.0",
  "private": true,
  "description": "伴学网",
  "scripts": {
    "test": "NODE_ENV=test mocha --require babel-polyfill --compilers js:babel-register --recursive -R list",
    "build-css": "rm -r -f public/css && mkdir -p public/css && gulp build-css",
    "build-js": "rm -r -f public/dist && webpack --display-error-details",
    "webpack": "webpack --watch",
    "dev": "npm run build-css && npm run build-js && npm start",
    "thrift": "bash ./sh/gen_thrift.sh",
    "start": "node bin/start_server.js",
    "valid-api": "swagger-tools validate spec/client_api.yml",
    "deploy-api": "js-yaml spec/client_api.yml > client_api.json && scp client_api.json ubuntu@172.16.0.130:/home/ubuntu/projects/swagger-doc/et-tutor-web/client_api.json && rm client_api.json",
    "patch-thrift": "bash ./sh/patch_thrift_0.9.3.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.easytech-main.com:webdev/et-tutor-web.git"
  },
  "author": "Wang BaLiang",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-register": "^6.18.0",
    "bluebird": "^3.4.6",
    "chokidar": "^1.6.1",
    "co": "^4.6.0",
    "config": "^1.21.0",
    "debug": "^2.2.0",
    "deep-equal": "^1.0.1",
    "generic-pool": "^3.1.2",
    "joi": "^10.0.1",
    "js-yaml": "^3.6.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-log4": "2.1.0",
    "koa-multer": "^1.0.0",
    "koa-nunjucks-next": "^1.1.1",
    "koa-redis": "^2.1.2",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "koa-xml-body": "^1.1.0",
    "lodash": "^4.16.4",
    "moment": "^2.17.1",
    "redis": "^2.6.3",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "thrift": "^0.9.3",
    "uuid": "^3.0.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.5.0",
    "eslint": "^3.7.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.13",
    "gulp-rev": "^7.1.2",
    "gulp-sourcemaps": "^2.2.0",
    "mocha": "^3.1.0",
    "style-loader": "^0.13.1",
    "swagger-tools": "^0.10.1",
    "webpack": "^1.13.3"
  }
}
